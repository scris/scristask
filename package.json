{
  "name": "scristask",
  "version": "1.10.0",
  "description": "Special Tasklist and Two Timers",
  "author": "Dominik Qiu <slaaker@126.com>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "build": "node build/build.js",
    "edev": "node build/build.electron.js && electron build/electron.js",
    "ewin": "node build/build.electron.js && electron-packager ./dist/ ScrisTask --platform=win32 --arch=x64 --icon=./src/assets/favicon.ico --overwrite",
    "emac": "node build/build.electron.js && electron-packager ./dist/ Task --platform=darwin --arch=x64 --icon=./src/assets/favicon.icns --overwrite",
    "elinux": "node build/build.electron.js && electron-packager ./dist/ ScrisTask --platform=linux --arch=x64 --icon=./src/assets/favicon.ico --overwrite",
    "cbrowser": "node build/build.app.js && cd app && cordova run browser && cd ../",
    "cios": "node build/build.app.js && cd app && cordova run ios && cd ../",
    "candroid": "node build/build.app.js && cd app && cordova run android && cd ../"
  },
  "dependencies": {
